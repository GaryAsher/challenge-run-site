{
  "ci": {
    "collect": {
      "staticDistDir": "./_site",
      "numberOfRuns": 1,
      "url": [
        "http://localhost/index.html",
        "http://localhost/games/index.html"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": "off",
        "categories:accessibility": ["warn", {"minScore": 0.8}],
        "categories:best-practices": "off",
        "categories:seo": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
